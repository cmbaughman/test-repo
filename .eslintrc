{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends":[ "eslint:recommended",
              "plugin:react/jsx-runtime"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
  },
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-unused-vars": "off"
  }
}
